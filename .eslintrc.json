{
  "extends": ["next/core-web-vitals", "eslint:recommended"],
  "plugins": ["prettier"],
  "rules": {
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "functions": "always-multiline"
  }],
    "curly": ["error"],
    "prettier/prettier":[
      "error", {
        "trailingComma": "all"
      }
    ],
    "no-unused-vars": "error",
    "no-undef": "error"
  }
}
